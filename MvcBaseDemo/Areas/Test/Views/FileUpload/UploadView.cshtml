@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>BaseMethod</title>
</head>
<body>
    <div>
        @using (Html.BeginForm("BaseMethod", "FileUpload", FormMethod.Post, new { enctype = "multipart/form-data" }))
        {
            <input name="uploadFile" type="file" />
            <input type="submit" value="Upload File" />
        }
    </div>
</body>
</html>
